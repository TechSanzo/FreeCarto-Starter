{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/favicon.ico",
      "headers": {
        "Cache-Control": "no-cache, no-store, must-revalidate"
      }
    },
    {
      "src": "/favicon-new.ico",
      "headers": {
        "Cache-Control": "public, max-age=31536000"
      }
    }
  ]
}
